{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "resources": [
        {
            "type": "Microsoft.ContainerRegistry/registries",
            "apiVersion": "2019-05-01",
            "name": "shellmacr9988",
            "location": "eastus",
            "sku": {
                "name": "Basic"
            },
            "properties": {
                "adminUserEnabled": true
            }
        }
    ],
    "outputs": {
        "loginServer": {
            "type": "string",
            "value": "[reference(resourceId('Microsoft.ContainerRegistry/registries', 'shellmacr9988')).loginServer]"
        },
        "username": {
            "type": "string",
            "value": "shellmacr9988"
        },
        "password": {
            "type": "securestring",
            "value": "[listCredentials(resourceId('Microsoft.ContainerRegistry/registries', 'shellmacr9988'), '2019-05-01').passwords[0].value]"
        }
    }
}